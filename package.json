{
  "name": "redux-express-starter",
  "version": "1.0.0",
  "description": "Simple starter package for Redux with React, Router, Boostrap Material and Babel support",
  "main": "index.js",
  "repository": "https://github.com/rgdalessandro/ReduxSimpleStarter.git",
  "scripts": {
    "postinstall": "webpack -p",
    "dev": "node ./node_modules/webpack-dev-server/bin/webpack-dev-server.js  --host 0.0.0.0",
    "start": "nodemon ./server/server.js",
    "test": "mocha --compilers js:babel-core/register --require ./test/test_helper.js --recursive ./test",
    "test:watch": "npm run test -- --watch"
  },
  "author": "Ricardo D'Alessandro",
  "license": "ISC",
  "devDependencies": {
    "babel-core": "^6.2.1",
    "babel-loader": "^6.2.0",
    "babel-preset-es2015": "^6.1.18",
    "babel-preset-es2016": "^6.16.0",
    "babel-preset-react": "^6.1.18",
    "babel-preset-stage-0": "^6.16.0",
    "chai": "^3.5.0",
    "chai-jquery": "^2.0.0",
    "jquery": "^2.2.1",
    "jsdom": "^8.1.0",
    "mocha": "^2.4.5",
    "nodemon": "^1.11.0",
    "react-addons-test-utils": "^0.14.7",
    "webpack": "^1.12.9",
    "webpack-dev-server": "^1.14.0"
  },
  "dependencies": {
    "babel-preset-stage-1": "^6.1.18",
    "lodash": "^3.10.1",
    "radium": "^0.18.1",
    "react": "^0.14.3",
    "react-cookie": "^0.4.8",
    "react-dom": "^0.14.3",
    "react-icons": "^2.2.1",
    "react-ios-switch": "^0.1.10",
    "react-redux": "^4.0.0",
    "react-router": "^2.0.1",
    "react-router-redux": "^4.0.6",
    "redux": "^3.0.4",
    "redux-logger": "^2.7.4",
    "redux-thunk": "^2.1.0"
